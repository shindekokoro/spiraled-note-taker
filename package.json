{
  "name": "spiraled-note-taker",
  "version": "1.0.0",
  "description": "Spiraled Note Taker replaces your old spiraled note book allowing you to take notes on the web using an express.js server to host the files.",
  "main": "server.js",
  "scripts": {
    "start": "pm2-runtime start ecosystem.config.js --env production &",
    "watch": "pm2 start server.js --name sNotes --watch",
    "stop": "pm2 stop sNotes",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shindekokoro/spiraled-note-taker.git"
  },
  "keywords": [
    "note"
  ],
  "author": "shindekokoro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shindekokoro/spiraled-note-taker/issues"
  },
  "homepage": "https://github.com/shindekokoro/spiraled-note-taker#readme",
  "dependencies": {
    "express": "^4.18.2",
    "pm2": "^5.3.0"
  }
}
